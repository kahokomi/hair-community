$(".test-follow #follow_<%= @user.id %>").html("<%= j(render "relationships/follow", user: @user) %>");
$(".follow-count").html("<%= j(render "relationships/link", user: @user) %>");
<% if current_page?(user_path) %>
  $(".userpage-tweet-follow").html("<%= j(render 'tweets/lists', tweets: @my_tweets) %>");
<% elsif current_page?(tweets_path) %>
  $("#tab1").html("<%= j(render 'tweets/lists', tweets: @hd_tweets) %>");
  $("#tab2").html("<%= j(render 'tweets/lists', tweets: @user_tweets) %>");
<% end %>